<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        xmlns:app="http://schemas.android.com/apk/res-auto">

    <fragment
            android:id="@+id/map"
            android:name="com.google.android.gms.maps.SupportMapFragment"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:context=".actionBarGroup.MapsActivity2" />


    <com.sa90.materialarcmenu.ArcMenu
            android:id="@+id/arcmenu_android_example_layout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            app:menu_color="@color/colorPrimary"
            app:menu_open="arc_right"
            app:menu_scr="@drawable/ic_map_dots">

        <com.google.android.material.floatingactionbutton.FloatingActionButton
                android:id="@+id/cod_no"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:clickable="true"
                android:focusable="true"
                android:onClick="onClick"
                app:backgroundTint="@color/colorPrimary"
                app:srcCompat="@drawable/name_map_events" />

        <com.google.android.material.floatingactionbutton.FloatingActionButton
                android:id="@+id/event_ov_location"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:clickable="true"
                android:focusable="true"
                android:onClick="onClick"
                app:backgroundTint="@color/colorPrimary"
                app:srcCompat="@drawable/name_map_food" />

        <com.google.android.material.floatingactionbutton.FloatingActionButton
                android:id="@+id/fab_arc_menu_3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:clickable="true"
                android:focusable="true"
                android:onClick="onClick"
                app:backgroundTint="@color/colorPrimary"
                app:srcCompat="@drawable/name_map_lodging" />

        <com.google.android.material.floatingactionbutton.FloatingActionButton
                android:id="@+id/fab_arc_menu_4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:clickable="true"
                android:focusable="true"
                android:onClick="onClick"
                app:backgroundTint="@color/colorPrimary"
                app:srcCompat="@drawable/name_map_atm" />

        <com.google.android.material.floatingactionbutton.FloatingActionButton
                android:id="@+id/fab_arc_menu_5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:clickable="true"
                android:focusable="true"
                android:onClick="onClick"
                app:backgroundTint="@color/colorPrimary"
                app:srcCompat="@drawable/name_map_venues" />
    </com.sa90.materialarcmenu.ArcMenu>


</RelativeLayout>
